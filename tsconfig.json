{
  "compilerOptions": {
    "allowJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["DOM", "WebWorker", "ES5", "ES6", "ES7", "ES2017"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ES5"
  },
  "include": ["src"]
}
